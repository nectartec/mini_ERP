    <div class="modal-dialog modal-sm" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">Meu Perfil</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <form is="dmx-serverconnect-form" id="serverconnectform1" method="post" action="/api/usuarios/atualizarUsuario" dmx-generator="bootstrap5" dmx-form-type="vertical" dmx-on:success="notifies.success('Dados atualizados com sucesso');modal_meu_perfil.hide();serverconnect1.load({})">
            <div class="row">
              <div class="col text-center">
                <img width="90" height="90" class="rounded-circle" dmx-bind:src="serverconnect1.data.query.avatar">
              </div>
            </div>
            <div class="form-group mb-3">
              <input id="inp_avatar" name="Avatar" type="file" class="form-control" dmx-bind:value="serverconnect1.data.query.avatar">
              <label for="inp_avatar">Avatar</label>
            </div>
            <div class="form-group mb-3">
              <label for="inp_nome">Nome</label>
              <input type="text" class="form-control" id="inp_nome" name="nome" aria-describedby="inp_nome_help" placeholder="Enter Nome" required="" data-msg-required="Este campo é obrigatorio" dmx-bind:value="serverconnect1.data.query.nome">
            </div>
            <div class="form-group mb-3">
              <label for="inp_senha">Senha</label>
              <input type="password" class="form-control" id="inp_senha" name="senha" aria-describedby="inp_senha_help" placeholder="Enter Senha" required="" data-msg-required="Este campo é obrigatorio">
            </div>
            <div class="form-group mb-3 text-xxl-center">
              <button type="submit" class="btn btn-primary" dmx-bind:disabled="state.executing">Atualizar <span class="spinner-border spinner-border-sm" role="status" dmx-show="state.executing"></span></button>
            </div>
          </form>
        </div>
      </div>
    </div>
